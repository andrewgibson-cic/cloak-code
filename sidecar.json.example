{
  "port": 8888,
  "env_file": ".env.vault",
  "routes": {
    "/openai": {
      "target": "https://api.openai.com/v1",
      "replace_values": ["OPENAI_API_KEY", "OPENAI_ORG_ID"]
    },
    "/stripe": {
      "target": "https://api.stripe.com/v1",
      "replace_values": ["STRIPE_SECRET_KEY"]
    }
  }
}
